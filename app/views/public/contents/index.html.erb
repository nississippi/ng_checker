<div class="container mt-5">
	<div class="row">
		<div class="offset-lg-2 col-lg-8 col-sm-12">
			<table class="table mt-3">
				<thead>
					<tr>
						<th>コンテンツ</th>
						<th>リリース</th>
					</tr>
				</thead>
				<tbody>
					<% @contents.each do |content| %>
						<tr>
							<td>
								<%= link_to content.title, content_path(content), class: "underline" %>
							</td>
							<td><%= content.release_date %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>